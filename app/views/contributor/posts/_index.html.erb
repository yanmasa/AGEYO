<h4 class="mt-5">投稿一覧</h4>
<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>Title</th>
      <th>ステータス</th>
      <th>お気に入り数</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% posts.each do |post| %>
      <tr>
        <td>
          <%= link_to post.title , contributor_post_path(post) %>
        </td>
        <td>
          <%= post.status %>
        </td>
        <td>
          <%= post.favorites.count %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>